{
  "name": "glimmer-hn",
  "version": "0.0.0",
  "description": "A brand new Glimmer app.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "start": "ember server --proxy=https://node-hnapi.herokuapp.com",
    "start:prod": "ember serve --environment=production --proxy=https://node-hnapi.herokuapp.com --live-reload=false",
    "test": "ember test",
    "bench": "node ./benching/index.js",
    "serve": "node --expose-gc ./benching/server.js"
  },
  "devDependencies": {
    "@glimmer/application": "^0.5.1",
    "@glimmer/application-pipeline": "^0.7.0",
    "@glimmer/component": "^0.4.0",
    "@glimmer/resolver": "^0.3.0",
    "chrome-tracing": "^0.9.0",
    "broccoli-asset-rev": "^2.5.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-cli": "github:ember-cli/ember-cli",
    "ember-cli-inject-live-reload": "^1.6.1",
    "express": "^4.8.5",
    "glob": "^7.1.1",
    "har-remix": "^1.2.0",
    "mkdirp": "^0.5.1",
    "morgan": "^1.3.2",
    "typescript": "^2.2.2"
  },
  "engines": {
    "node": ">= 4.0"
  },
  "private": true,
  "dependencies": {
    "route-recognizer": "^0.3.3"
  }
